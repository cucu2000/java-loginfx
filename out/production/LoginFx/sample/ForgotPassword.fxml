<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="185.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ForgotPasswordController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Forgot Password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="67.0" text="Email" />
      <TextField fx:id="txtEmail" layoutX="32.0" layoutY="84.0" prefHeight="25.0" prefWidth="281.0" />
      <Button fx:id="btnSend" layoutX="69.0" layoutY="123.0" mnemonicParsing="false" onAction="#send" text="Send" />
      <Button fx:id="btnCancel" layoutX="228.0" layoutY="123.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
